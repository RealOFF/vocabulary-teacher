{
  "name": "vocabulary-teacher-0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "format": "pnpm -r format",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "dev": "pnpm -r dev",
    "build": "pnpm -r build",
    "check-types": "pnpm -r check-types",
    "dev:native": "pnpm --filter native dev",
